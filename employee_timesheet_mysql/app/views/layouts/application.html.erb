<!DOCTYPE html>
<html>
  <head>
    <title>EmployeeTimesheetMysql</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <% if notice %>
      <div>
        <div class='success'><%= notice %></div>
      </div>
    <% end %>

    <% if alert %>
      <div>
        <div class='alert'><%= alert %></div>
      </div>
    <% end %>
    <%= yield %>

  </body>
</html>
